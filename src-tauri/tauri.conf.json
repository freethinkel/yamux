{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "pnpm build",
		"beforeDevCommand": "pnpm dev",
		"devPath": "http://localhost:3000",
		"distDir": "../dist",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "Yamux",
		"version": "0.1.5"
	},
	"tauri": {
		"macOSPrivateApi": true,
		"allowlist": {
			"all": true,
			"http": {
				"all": true,
				"request": true,
				"scope": ["https://**"]
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "ru.freethinkel.yamux",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"title": "Yamux",
				"width": 800,
				"minWidth": 200,
				"minHeight": 200,
				"height": 600,
				"resizable": true,
				"fullscreen": false,
				"transparent": true
			}
		]
	}
}
